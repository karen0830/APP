            <div class="stories-img">
                <img src="/public/img/8.jpeg" alt="">
            </div>
        </div>
        <div class="feed">
            <h1>Feed</h1>
            <div class="feed-text">
                <h2>Latest</h2>
                <span>Populer</span>
            </div>
        </div>
